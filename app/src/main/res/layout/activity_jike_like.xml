<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.MainActivity">

    <include
        layout='@layout/include_layout_toolbar'
        />

    <com.example.customview.jike.JiKeLikeView
        android:id="@+id/jikeone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        app:like_number="999"/>
    <com.example.customview.jike.JiKeLikeView
        android:id="@+id/jiketwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="10dp"
        app:like_number="1000"/>
    <com.example.customview.jike.JiKeLikeView
        android:id="@+id/jikethree"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        app:like_number="104"/>
    <com.example.customview.jike.JiKeLikeView
        android:id="@+id/jikefour"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="10dp"
        app:like_number="34"/>
    <com.example.customview.jike.JiKeLikeView
        android:id="@+id/jikefive"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="10dp"
        app:like_number="4309"/>

</LinearLayout>